<!DOCTYPE html>
<html lang="en">

<head itemscope itemtype="http://schema.org/Article">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta itemprop="name" content="">
    <meta itemprop="description" content="">
    <meta itemprop="image" content="">

    <meta prefix="og: http://ogp.me/ns#" property="og:site_name" content="" />
    <meta prefix="og: http://ogp.me/ns#" property="og:title" content="" />
    <meta prefix="og: http://ogp.me/ns#" property="og:type" content="article" />
    <meta prefix="og: http://ogp.me/ns#" property="og:description" content="" />
    <meta prefix="og: http://ogp.me/ns#" property="og:url" content="" />
    <meta prefix="og: http://ogp.me/ns#" property="og:image" content="" />

    <meta name="title" content="" />
    <meta name="description" content="{% block description %}{% endblock %}" />

    <title>{% block title %}{% endblock %}</title>

    {% block StyleSheets %}
        <link href="{{ asset('bundles/sl24/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
    {% endblock %}
</head>

<body ng-app="Sl24">
    {% block page %}

        {% block body %}

        {% endblock %}

    {% endblock %}

    {% block JavaScripts %}
        <script src="{{ asset('bundles/sl24/js/jQuery/jquery-2.1.3.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sl24/js/Bootstrap/bootstrap.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sl24/js/AngularJS/angular.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sl24/js/AngularJS/angular-route.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sl24/js/AngularJS/angular-sanitize.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sl24/js/AngularJS/angular-animate.min.js') }}" type="text/javascript"></script>

        <script src="{{ asset('bundles/sl24/js/Custom/core.js') }}" type="text/javascript"></script>
        <script>
            URLS = {
                'asset': "{{ asset('') }}"
            };
        </script>
    {% endblock %}
</body>
</html>